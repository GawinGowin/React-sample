{
    "version": "0.2.0",
    "configurations": [
        {
        "name": "Debug React",
        "type": "node",
        "request": "launch",
        "runtimeExecutable": "node",
        "cwd": "${workspaceFolder}/react-sample",
        "program": "${workspaceFolder}/react-sample/node_modules/react-scripts/bin/react-scripts.js",
        "args": [
            "start"
        ],
        "console": "integratedTerminal",
        "internalConsoleOptions": "neverOpen",
        "skipFiles": [
            "<node_internals>/**"
        ],
        "outFiles": [
            "${workspaceFolder}/react-sample/build/**/*.js"
        ],
        "sourceMaps": true,
        },
    ]
  }